<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="500.0" prefWidth="500.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mokujin.controllers.WelcomeController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox prefHeight="500.0" prefWidth="502.0" style="-fx-background-color: #e6e6e6;">
         <children>
            <Label alignment="CENTER" text="Welcome!" textFill="#112380">
               <VBox.margin>
                  <Insets bottom="10.0" left="135.0" right="10.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="Castellar" size="40.0" />
               </font>
            </Label>
            <TextField fx:id="usernameTF" prefHeight="35.0" prefWidth="300.0" promptText="username" style="-fx-border-radius: 10px;">
               <VBox.margin>
                  <Insets bottom="5.0" left="100.0" right="100.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria" size="12.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordTF" prefHeight="35.0" prefWidth="300.0" promptText="password">
               <VBox.margin>
                  <Insets bottom="5.0" left="100.0" right="100.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria" size="12.0" />
               </font>
            </PasswordField>
            <Label fx:id="firstLabel" textFill="RED">
               <VBox.margin>
                  <Insets left="160.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="secondLabel" textFill="RED">
               <VBox.margin>
                  <Insets left="205.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="thirdLabel" textFill="RED">
               <VBox.margin>
                  <Insets bottom="10.0" left="190.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#logIn" prefHeight="27.0" prefWidth="137.0" style="-fx-background-color: #112380; -fx-background-radius: 15px;" text="Log In" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="10.0" left="180.0" right="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria" size="16.0" />
               </font>
               <padding>
                  <Insets bottom="2.0" left="35.0" right="35.0" top="3.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#getDetails" prefHeight="31.0" prefWidth="138.0" style="-fx-background-color: #112380; -fx-background-radius: 15px;" text="About Me" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="10.0" left="180.0" right="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Cambria" size="16.0" />
               </font>
               <padding>
                  <Insets bottom="2.0" left="35.0" right="35.0" top="3.0" />
               </padding>
            </Button>
            <Label fx:id="informationLabel" textFill="#113680">
               <font>
                  <Font name="Cambria" size="17.0" />
               </font>
               <VBox.margin>
                  <Insets left="100.0" right="20.0" top="130.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="informationLabelTwo" textFill="#113680">
                     <font>
                        <Font name="Cambria" size="17.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="180.0" />
                     </HBox.margin>
                  </Label>
                  <Hyperlink fx:id="link">
                     <font>
                        <Font name="Cambria" size="16.0" />
                     </font>
                  </Hyperlink>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</GridPane>
